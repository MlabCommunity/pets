{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Postgres": {
    "ConnectionString": "Password=postgres;Host=host.docker.internal;Port=5441;Database=posxtgresPet;Username=postgres"
  },
  "gRPC": {
    "IdentityServerAddress": "http://host.docker.internal:5011"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://host.docker.internal:5040",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "http://host.docker.internal:5041",
        "Protocols": "Http2"
      }
    }
  },
  "rabbitMq": {
    "connectionName": "rabbitPet",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "rabbitmq",
    "password": "rabbitmq",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "host.docker.internal"
    ],
    "exchange": {
      "name": "pet"
    },
    "queue": {
      "template": "pet-service/{{exchange}}.{{message}}"
    }
  }
}
