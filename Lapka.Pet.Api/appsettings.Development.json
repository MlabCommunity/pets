{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Postgres": {
    "ConnectionString": "Password=postgres;Host=localhost;Port=5441;Database=postgresPet34;Username=postgres"
  },
  "JWT": {
    "issuer": "http://localhost:5012",
    "rsaPublicKeyPath": "rsa-public-key.pem",
    "clockSkew": "00:00:00"
  },

  "gRPC": {
    "IdentityServerAddress": "http://localhost:5011"
  },

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5040",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "http://localhost:5041",
        "Protocols": "Http2"
      }
    }
  },

  "rabbitMq": {
    "connectionName": "rabbitPet",
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "rabbitmq",
    "password": "rabbitmq",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "exchange": {
      "name": "pet"
    },
    "queue": {
      "template": "pet-service/{{exchange}}.{{message}}"
    }
  }
}
